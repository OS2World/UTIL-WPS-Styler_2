// copertina
DLGTEMPLATE DLG_COVER LOADONCALL MOVEABLE DISCARDABLE
BEGIN
    DIALOG  SZ_NULL, DLG_COVER, 0, 0, CX_PAGEDLG, CY_PAGEDLG, NOT FS_DLGBORDER |
            WS_VISIBLE
    BEGIN
      CONTROL SZLOGONAME, WSBMP_LOGO, 0, 0, 0, 0, WC_EDLGSIMG,
              WS_VISIBLE | ESI_BMP
      CONTROL SZ_NULL, TXT_VERSION, 0, 0, 0, 0, WC_STATUSBAR,
              WS_VISIBLE | DT_CENTER | DT_VCENTER | CCTL_BORDER2 | CCTL_RAISED
              PRESPARAMS PP_FONTNAMESIZE SZ_FONTTITLE
    END
END


// pagina exceptions (comune x le varie opzioni ) -------------------------

DLGTEMPLATE DLG_EXCEPTLIST LOADONCALL MOVEABLE DISCARDABLE
BEGIN
    DIALOG  SZ_NULL, DLG_EXCEPTLIST, 0, 0, CX_PAGEDLG, CY_PAGEDLG, NOT FS_DLGBORDER |
            WS_VISIBLE
            PRESPARAMS PP_FONTNAMESIZE SZ_FONTDLG
    BEGIN
        LTEXT           SZ_EXCPTLIST, TXT_EXCPLIST, 2, 138, 216, 8,
                        DT_VCENTER | NOT WS_GROUP
        LISTBOX         LBOX_EXCEPTION, 2, 30, 216, 108,
                        LS_NOADJUSTPOS | WS_GROUP | WS_TABSTOP | LS_EXTENDEDSEL
        ENTRYFIELD      SZ_NULL, EF_LISTITEM, 4, 18, 212, 8, WS_TABSTOP |
                        ES_LEFT | ES_AUTOSCROLL | ES_MARGIN | WS_GROUP
                        CTLDATA 8, 255, 0, 0
        PUSHBUTTON      SZ_FIND, BTN_EXCEPTFIND, 2, 2, 67, 13,
                        WS_GROUP | WS_TABSTOP
        PUSHBUTTON      SZ_ADD, BTN_EXCEPTCADD, 70, 2, 67, 13,
                        NOT WS_TABSTOP | WS_DISABLED
        PUSHBUTTON      SZ_REMOVE, BTN_EXCECPTREM, 138, 2, 67, 13,
                        NOT WS_TABSTOP | WS_DISABLED
    END
END


// dialogo selezione azioni mouse e tastiera (finestra indipendente)

DLGTEMPLATE DLG_MOUKEYBACTSFL LOADONCALL MOVEABLE DISCARDABLE
BEGIN
    DIALOG  SZ_NULL, DLG_MOUKEYBACTSFL, 0, 0, CX_PAGEDLG, CY_PAGEDLG,
            NOT WS_SYNCPAINT | NOT WS_VISIBLE, FCF_SYSMENU | FCF_TITLEBAR |
            FCF_MINBUTTON | FCF_MAXBUTTON | FCF_SIZEBORDER
            PRESPARAMS PP_FONTNAMESIZE SZ_FONTDLG
    BEGIN
        LTEXT           SZ_FEFUNCEVENT, TXT_FEFNCTEVNTS, 2, 145, 119, 8,
                        DT_MNEMONIC
        LISTBOX         LBOX_FEFNCTEVNTS, 2, 68, 216, 76, LS_NOADJUSTPOS
        CONTROL         SZ_NULL, SBAR_FEFNCTEVNT, 2, 56, 216, 10, WC_STATUSBAR,
                        WS_VISIBLE | CCTL_BORDER1
        AUTOCHECKBOX    SZ_ENABLE, CHK_FEENABLE, 2, 42, 44, 10,
                        WS_GROUP | WS_DISABLED
        LTEXT           SZ_PRIMKEY, TXT_FEKEYBTN, 46, 42, 64, 10,
                        DT_MNEMONIC | DT_VCENTER | WS_DISABLED
        SPINBUTTON      SPIN_FEFKEYBTN, 110, 42, 46, 12, SPBS_READONLY |
                        SPBS_MASTER | SPBS_JUSTLEFT | WS_DISABLED
        AUTORADIOBUTTON SZ_CLICK, RDBTN_FECLICK, 46, 32, 99, 10, WS_GROUP |
                        WS_TABSTOP | NOT WS_VISIBLE
        AUTORADIOBUTTON SZ_DBLCLK, RDBTN_FEDBLCLK, 46, 22, 99, 10,
                        NOT WS_VISIBLE
        AUTOCHECKBOX    SZ_CTRL, CHK_FECTRL, 169, 42, 49, 10, WS_GROUP |
                        WS_DISABLED
        AUTOCHECKBOX    SZ_ALT, CHK_FEALT, 169, 32, 49, 10, NOT WS_TABSTOP |
                        WS_DISABLED
        AUTOCHECKBOX    SZ_SHIFT, CHK_FESHIFT, 169, 22, 49, 10,
                        NOT WS_TABSTOP  | WS_DISABLED
        CONTROL         SZ_NULL, GRP_FEHBAR, 2, 17, 216, 2, WC_BAR, WS_VISIBLE
        PUSHBUTTON      SZ_OK, BTN_FEOK, 2, 2, 60, 13, WS_GROUP | WS_TABSTOP |
                        WS_DISABLED
        PUSHBUTTON      SZ_CANCEL, BTN_FECANCEL, 64, 2, 60, 13, WS_GROUP
        PUSHBUTTON      SZ_HELP, BTN_FEHELP, 126, 2, 60, 13, WS_GROUP |
                        WS_TABSTOP
    END
END


