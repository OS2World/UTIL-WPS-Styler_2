// dialogo selezione bitmap -----------------------------------------------

DLGTEMPLATE DLG_BMPSEL LOADONCALL MOVEABLE DISCARDABLE
BEGIN
    DIALOG  "", DLG_BMPSEL, 0, 0, 0, 0, //BMPSELDLG_CX, BMPSELDLG_CY,
            NOT FS_DLGBORDER |
            FS_SIZEBORDER | NOT WS_VISIBLE, FCF_SYSMENU | FCF_TITLEBAR |
            FCF_MINBUTTON | FCF_MAXBUTTON
            PRESPARAMS PP_FONTNAMESIZE SZ_FONTDLG
    BEGIN
        CONTROL         "", LBOX_BMPSEL, 1, 1, 157, 113, WC_LISTBOX,
                        LS_OWNERDRAW | LS_NOADJUSTPOS | WS_GROUP | WS_TABSTOP
                        PRESPARAMS PP_BACKGROUNDCOLORINDEX CLR_PALEGRAY
                        PRESPARAMS PP_DISABLEDBACKGROUNDCOLORINDEX CLR_PALEGRAY
        CONTROL         SZ_EDIT, BTN_EDITBMP, 159, 101, 76, 13, WC_BUTTON,
                        BS_PUSHBUTTON | WS_GROUP | WS_VISIBLE
        CONTROL         SZ_IMPORT___, BTN_FINDBMP, 159, 87, 76, 13,WC_BUTTON,
                        WS_GROUP | WS_TABSTOP | WS_VISIBLE | BS_PUSHBUTTON
        CONTROL         SZ_DISABLE, CHK_BMPDISABLE, 159, 70, 76, 10,WC_BUTTON,
                        WS_GROUP | WS_TABSTOP | NOT WS_VISIBLE | BS_AUTOCHECKBOX
        CONTROL         SZ__DEFAULT, CHK_BMPDEFAULT, 159, 60, 76, 10,WC_BUTTON,
                        WS_GROUP | WS_TABSTOP | NOT WS_VISIBLE | BS_AUTOCHECKBOX
        // combobox scelta posizione bottone chiusura
        CONTROL         "", COMBO_CLOSEPOS, 159, 13, 76, 38, WC_COMBOBOX,
                        CBS_DROPDOWNLIST | WS_TABSTOP | NOT WS_VISIBLE
        CONTROL         SZ_OK, BTN_BMPOK, 159, 29, 76, 13, WC_BUTTON,
                        WS_GROUP | WS_TABSTOP | WS_VISIBLE | BS_PUSHBUTTON |
                        BS_DEFAULT
        CONTROL         SZ_CANCEL, BTN_BMPCANCEL, 159, 15, 76, 13, WC_BUTTON,
                        WS_VISIBLE | BS_PUSHBUTTON
        CONTROL         SZ_HELP, BTN_BMPHELP, 159, 1, 76, 13, WC_BUTTON,
                        WS_VISIBLE | BS_PUSHBUTTON
    END
END
